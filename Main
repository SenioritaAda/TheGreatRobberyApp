package TheGreatRobbery;

public class GreatRobberyApp {

    public static void main(String[] args) {
	City city = new City();
    Gang gang = new Gang();
    Police police = new Police();

    gang.getGangInfo();
    do{
        gang.letsRob(city.getBuildings());
    }while(!police.catchCriminals(gang));
    }
}
